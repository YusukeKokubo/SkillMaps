<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style type='com.appspot.skillmaps.client.ui.SkillAddDialog.SkillAddStyle'>
		.form {
			border: 2px solid #ABB8C7;
			width: 760px;
			padding: 15px;
			background-color: #ffffff;
		}
	</ui:style>
	<g:VerticalPanel>
		<g:Label text="スキルの名前" />
		<g:SuggestBox ui:field="skillName" />
		<g:HorizontalPanel>
			<g:Button ui:field="submit" text="submit" />
			<g:Button ui:field="cancel" text="cancel" />
		</g:HorizontalPanel>
	</g:VerticalPanel>

</ui:UiBinder>